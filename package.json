{
  "name": "@kibibit/branch-it",
  "version": "1.4.0",
  "description": "updates branch references in package.json",
  "main": "bin/branch-it.js",
  "bin": {
    "branch-it": "bin/branch-it.js"
  },
  "scripts": {
    "update:dep": "npm-check",
    "test": "echo \"Error: no test specified\" && exit 0",
    "semantic-release": "semantic-release",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "update-branch": "node ./bin/branch-it.js",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "author": "neilkalman@gmail.com",
  "license": "MIT",
  "dependencies": {
    "command-line-args": "^5.0.2",
    "current-git-branch": "^1.1.0",
    "find-up": "^3.0.0",
    "fs-extra": "^7.0.1",
    "lodash": "^4.17.11",
    "manakin": "^0.5.2",
    "prettyjson": "^1.2.1",
    "snyk": "^1.189.0"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/git": "^7.1.0-beta.3",
    "@semantic-release/github": "^5.2.10",
    "@semantic-release/npm": "^5.1.4",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "all-contributors-cli": "^6.1.2",
    "npm-check": "^5.9.0",
    "semantic-release": "^16.0.0-beta.18",
    "semantic-release-cli": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kibibit/branch-it.git"
  },
  "release": {
    "branches": [
      "+([1-9])?(.{+([1-9]),x}).x",
      "master",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "next",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ],
    "versionMapping": {
      "master": "stable",
      "develop": "next",
      "next": "next"
    }
  },
  "publishConfig": {
    "tag": "stable"
  },
  "snyk": true
}
